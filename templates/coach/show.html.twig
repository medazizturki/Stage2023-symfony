{% extends 'base.html.twig' %}

{% block title %}Coach{% endblock %}

{% block body %}
    <h1>Coach</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ coach.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ coach.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ coach.prenom }}</td>
            </tr>
            <tr>
                <th>Sexe</th>
                <td>{{ coach.sexe }}</td>
            </tr>
            <tr>
                <th>Age</th>
                <td>{{ coach.age }}</td>
            </tr>
            <tr>
                <th>Contrat</th>
                <td>{{ coach.contrat }}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{{ coach.datedebut ? coach.datedebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{{ coach.datefin ? coach.datefin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ coach.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ coach.roles ? coach.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ coach.password }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_coach_index') }}">back to list</a>

    <a href="{{ path('app_coach_edit', {'id': coach.id}) }}">edit</a>

    {{ include('coach/_delete_form.html.twig') }}
{% endblock %}
